<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yijia360.pay.core.dao.partner.PartnerAppDao">
	
	<select id="get" parameterType="PartnerApp" resultType="PartnerAppVo">
		SELECT * FROM pay_partner_app
		<where>
			<if test="appId != null">
				AND appId = #{appId}
			</if>
			<if test="partnerId !=null">
				AND partnerId = #{partnerId}
			</if>
		</where>
		LIMIT 1
	</select>
	
</mapper>